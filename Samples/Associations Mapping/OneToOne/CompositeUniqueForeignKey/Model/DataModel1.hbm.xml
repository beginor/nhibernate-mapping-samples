<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="CompositeUniqueForeignKey" namespace="CompositeUniqueForeignKey" xmlns="urn:nhibernate-mapping-2.2">
  <class name="Person" table="Person">
    <id name="PersonID" type="Int32">
      <column name="PersonID" not-null="true" sql-type="INTEGER" />
      <generator class="identity" />
    </id>
    <property name="BirthDay" type="DateTime">
      <column name="BirthDay" not-null="true" sql-type="DATE" />
    </property>
    <property name="Address" type="String">
      <column name="Address" not-null="true" length="200" sql-type="VARCHAR" />
    </property>
    <many-to-one name="Contact" class="Contact">
      <column name="FirstName" not-null="true" length="50" sql-type="VARCHAR" />
      <column name="LastName" not-null="true" length="50" sql-type="VARCHAR" />
    </many-to-one>
  </class>
  <class name="Contact" table="Contact">
    <composite-id>
      <key-property name="FirstName" type="String">
        <column name="FirstName" not-null="true" length="50" sql-type="VARCHAR" />
      </key-property>
      <key-property name="LastName" type="String">
        <column name="LastName" not-null="true" length="50" sql-type="VARCHAR" />
      </key-property>
    </composite-id>
    <property name="Phone" type="String">
      <column name="Phone" not-null="false" sql-type="Ð¡HAR" />
    </property>
    <one-to-one name="Person" class="Person" property-ref="Contact" />
  </class>
</hibernate-mapping>